public class ProgramaComBreak {
    public static void main(String args[]) {
        for(int linha = 0; linha < 6; linha++) {
            for (int coluna = 0; coluna < 6; coluna++) {
                if ( coluna > linha ) {
                    break;
                }
                System.out.print( coluna );
            }
            System.out.println();
        }
    }
}